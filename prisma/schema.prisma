generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Todo {
  id Int @id @default(autoincrement())
  description String
  status   Status @default(NEW)
}

enum Status {
  NEW
  PENDING
  COMPLETE
}
